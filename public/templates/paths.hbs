<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Configured routes</h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Path</th>
          <th>Limit</th>
          <th>Duration</th>
        </tr>
      </thead>
      <tbody>
        {{#each item in model}}
          <tr class="path">
            <td>{{item.fragment}}</td>
            <td>{{item.limit}}</td>
            <td>{{item.seconds}}s</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>


<div id="pathsOutlet">
  {{outlet}}
</div>

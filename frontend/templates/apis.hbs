<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Configured Apis</h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Path</th>
          <th>App</th>
        </tr>
      </thead>
      <tbody>
        {{#each item in apisToShow}}
          <tr class="path">
            <td>{{item.fragment}}</td>
            <td>{{item.app.name}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>


<div id="apisOutlet">
  {{outlet}}
</div>
